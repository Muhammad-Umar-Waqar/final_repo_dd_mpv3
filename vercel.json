{
    "version": 2,
    "builds": [
      {"src": "*.js", "use": "@vercel/node"}
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/"
      }
    ],
    "redirects": [
      {
        "source": "/consejos/:path*",
        "destination": "/",
        "permanent": true
      },
      {
        "source": "/recetas/:path*",
        "destination": "/",
        "permanent": true
      }
    ]
  }

